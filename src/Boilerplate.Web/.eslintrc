{
  "parser": "@babel/eslint-parser",
  "plugins": ["react"],
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "settings": {
    "import/resolver": {
      "node": {},
      "webpack": {}
    },
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "max-len": ["error", { "code": 150 }]
  }
}
